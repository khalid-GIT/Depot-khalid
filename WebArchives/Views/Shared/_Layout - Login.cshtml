<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <!--CSS-->
    @Styles.Render("~/Content/bootstrap/css")
    @Styles.Render("~/Content/metisMenu/css")
    @Styles.Render("~/Content/timeline/css")
    @Styles.Render("~/Content/startmin/css")
    @Styles.Render("~/Content/morris/css")
    @Styles.Render("~/Content/fontawesome/css")
    <!--JS-->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/raphael/js")
    @Scripts.Render("~/bundles/metisMenu/js")
    @Scripts.Render("~/bundles/jqueryval")


    @*@Scripts.Render("~/bundles/startmin/js")*@
    @if (IsSectionDefined("scripts"))
    {
        @RenderSection("scripts", required: false);
    }

    @* @RenderSection("scripts", required: false)*@
</head>
<body>
    <div id="wrapper">
        
        <div id="page-wrapper">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
    </div>
</body>
</html>